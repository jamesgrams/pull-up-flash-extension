{
    "name": "Pull Up Flash",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Click to open flash content on web pages in Flash Player",
    "homepage_url": "https://game103.net",
    "browser_action": {
        "default_title": "Pull Up Flash"
    },
    "permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        },
        {
            "matches": [
                "*://*/*"
            ],
            "css": ["content.css"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "icon16.png",
        "48": "icon32.png",
        "138": "icon128.png"
    }
}